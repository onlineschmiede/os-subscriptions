<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="OsSubscriptions\Checkout\Cart\RentDiscountProcessor">
            <argument type="service" id="Shopware\Core\Checkout\Cart\Price\PercentagePriceCalculator"/>
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService"/>
            <argument type="service" id="order.repository"/>
            <tag name="shopware.cart.processor" priority="4500" />
            <tag name="shopware.cart.collector" priority="4500" />
        </service>

        <service id="OsSubscriptions\Service\StockStorageDecorator" decorates="Shopware\Core\Content\Product\Stock\StockStorage">
            <argument type="service" id="OsSubscriptions\Service\StockStorageDecorator.inner" />
            <argument type="service" id="order.repository"/>
            <argument type="service" id="order_line_item.repository"/>
        </service>
    </services>
</container>
